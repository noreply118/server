<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Main\RatetradeBundle\Entity\EmailSubscription" table="email_subscription">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fname" type="string" column="fname" length="255" nullable="true"/>
    <field name="lname" type="string" column="lname" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="false"/>
    <field name="phone" type="string" column="phone" length="20" nullable="true"/>
    <field name="bestTime" type="string" column="best_time" length="255" nullable="true"/>
    <field name="location" type="string" column="location" length="255" nullable="true"/>
    <field name="purpose" type="string" column="purpose" length="255" nullable="true"/>
    <field name="message" type="text" column="message" nullable="true"/>
    <field name="updateDateTime" type="datetime" column="update_date_time" nullable="false"/>
  </entity>
</doctrine-mapping>
