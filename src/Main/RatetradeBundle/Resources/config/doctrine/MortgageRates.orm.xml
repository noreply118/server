<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Main\RatetradeBundle\Entity\MortgageRates" table="mortgage_rates">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rate" type="float" column="rate" precision="10" scale="0" nullable="false"/>
    <field name="termid" type="integer" column="termid" nullable="false"/>
    <field name="rateTypeid" type="integer" column="rate_typeid" nullable="false"/>
    <field name="cityid" type="integer" column="cityid" nullable="false"/>
  </entity>
</doctrine-mapping>
